{
    "name": "aios/skeleton",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.0",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "academe/omnipay-datatrans": "~3.0",
        "advoor/nova-editor-js": "*",
        "barryvdh/laravel-cors": "1.0.1",
        "barryvdh/laravel-ide-helper": "^2.6",
        "barryvdh/laravel-omnipay": "0.3.*@dev",
        "centrifugal/phpcent": "~3.0",
        "doctrine/dbal": "^2.9",
        "ebess/advanced-nova-media-library": "3.0.*",
        "eminiarts/nova-tabs": "^1.2.2",
        "epartment/nova-dependency-container": "^1.2",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.0",
        "kalnoy/nestedset": "^5.0",
        "laravel/cashier": "10.0.*",
        "laravel/framework": "^7.0",
        "laravel/nova": "*",
        "laravel/passport": "^7.3",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "laravelium/sitemap": "*",
        "league/omnipay": "^3",
        "mll-lab/graphql-php-scalars": "^2.1",
        "mll-lab/laravel-graphql-playground": "^1.1",
        "nexmo/client": "^1.9.0",
        "nuwave/lighthouse": "^4.5",
        "nwidart/laravel-modules": "^5.0",
        "optimistdigital/nova-sortable": "^1.3",
        "php-ffmpeg/php-ffmpeg": "^0.16.0",
        "plivo/plivo-php": "^4.9",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.2",
        "santigarcor/laratrust": "^5.2",
        "sentry/sentry-laravel": "1.7.*",
        "skidplay/user-top-info": "*",
        "spatie/laravel-medialibrary": "8.0.0",
        "spatie/laravel-translatable": "^4.2",
        "spatie/nova-translatable": "^2.0",
        "stevebauman/location": "^5.0",
        "tightenco/parental": "0.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "haydenpierce/class-finder": "^0.4.0",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600,
        "github-protocols": ["https"]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/academe/omnipay-datatrans"
        },
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/UserTopInfo"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
